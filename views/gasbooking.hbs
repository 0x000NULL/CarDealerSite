    {{!-- Contact Form --}}
    <div id="contactModal" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Get in Touch with our Agent</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group">
                            <label class="col-form-label">Name</label>
                            <input type="text" class="form-control" id="username" placeholder="John">
                        </div>

                        <div class="form-group">
                            <label>Email address</label>
                            <input type="email" class="form-control" id="useremail" placeholder="name@gmail.com">
                        </div>

                        <div class="form-group">
                            <label>Phone</label>
                            <input type="tel" class="form-control" id="userphone" placeholder="9381046426"
                                pattern="[0-9]{10}" required>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" id="contactSubmit" class="btn btn-secondary">Submit</button>
                </div>
            </div>
        </div>
    </div>



